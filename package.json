{
  "name": "cm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node -r esm ./bin/www",
    "start-dev": "nodemon -r esm -L --ignore public/ --ignore frontend/ --ignore donnees/ --ignore webpack.config.js ./bin/www",
    "build": "webpack --watch --mode development --config webpack.config.js",
    "build-once": "webpack --mode production --config webpack.config.js",
    "deploy": "time heroku container:push web --app couleurs-manifestes && heroku container:release web --app couleurs-manifestes",
    "deploy-staging": "time heroku container:push web --app couleurs-manifestes-staging && heroku container:release web --app couleurs-manifestes-staging"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "bluebird": "^3.7.2",
    "constantinople": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "express-flash-messages": "^0.1.1",
    "express-session": "^1.17.1",
    "google-auth-library": "^4.2.5",
    "googleapis": "^41.0.0",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "morgan": "~1.9.1",
    "needle": "^2.5.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "~2.0.4",
    "regenerator-runtime": "^0.13.7",
    "ua-parser-js": "^0.7.21",
    "vue-async-computed": "^3.8.3",
    "vue2-touch-events": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "whatwg-fetch": "^3.4.0"
  }
}
