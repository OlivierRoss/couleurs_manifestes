{
  "name": "cm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node -r esm ./bin/www",
    "start-dev": "nodemon -r esm -L --ignore public/ --ignore frontend/ ./bin/www",
    "build": "webpack --watch --mode development --config webpack.config.js",
    "build-once": "webpack --mode production --config webpack.config.js",
    "deploy": "time heroku container:push web --app couleurs-manifestes && heroku container:release web --app couleurs-manifestes"
  },
  "dependencies": {
    "bluebird": "^3.7.1",
    "constantinople": "^4.0.1",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "express-flash-messages": "^0.1.1",
    "express-session": "^1.17.0",
    "google-auth-library": "^4.2.5",
    "googleapis": "^41.0.0",
    "http-errors": "~1.6.3",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "~2.0.4",
    "ua-parser-js": "^0.7.20",
    "vue2-touch-events": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "core-js": "^3.3.3",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.4",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "whatwg-fetch": "^3.0.0"
  }
}
